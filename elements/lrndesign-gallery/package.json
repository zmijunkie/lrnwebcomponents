{
  "name": "@lrnwebcomponents/lrndesign-gallery",
  "wcfactory": {
    "className": "LrndesignGallery",
    "customElementClass": "LitElement",
    "elementName": "lrndesign-gallery",
    "generator-wcfactory-version": "0.7.1",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/lrndesign-gallery.css",
      "scss": "src/lrndesign-gallery.scss",
      "html": "src/lrndesign-gallery.html",
      "js": "src/lrndesign-gallery.js",
      "properties": "src/lrndesign-gallery-properties.json",
      "hax": "src/lrndesign-gallery-hax.json"
    },
    "sharedStyles": [
      "...super.styles"
    ]
  },
  "version": "4.0.15",
  "description": "displays images as a carousel or grid with the ability to zoom",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "lrndesign-gallery.js",
  "module": "lrndesign-gallery.js",
  "umd": "lrndesign-gallery.umd.js",
  "scripts": {
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && node --experimental-json-modules ../../node_modules/.bin/rollup --config && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"**/*.js\" --format vscode --outFile vscode-html-custom-data.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "web-dev-server -c ../../web-dev-server.config.js",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs",
    "test:watch": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox --watch",
    "test": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/img-view-modal": "^4.0.15",
    "@lrnwebcomponents/responsive-utility": "^4.0.15",
    "@lrnwebcomponents/simple-colors": "^4.0.15",
    "@lrnwebcomponents/simple-icon": "^4.0.15",
    "@lrnwebcomponents/simple-tooltip": "^4.0.15",
    "lit": "^2.2.0"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^4.0.0",
    "@lrnwebcomponents/utils": "^4.0.8",
    "@open-wc/testing": "3.0.4",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@web/dev-server": "0.1.30",
    "@webcomponents/webcomponentsjs": "2.6.0",
    "concurrently": "7.0.0",
    "gulp-babel": "8.0.0",
    "lodash": "^4.17.21",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html",
    "haxtheweb"
  ],
  "gitHead": "6003b73631b580af24e0b306cb72dae0ea26f506"
}
