{
  "name": "@lrnwebcomponents/simple-toolbar",
  "wcfactory": {
    "className": "SimpleToolbar",
    "customElementClass": "LitElement",
    "elementName": "simple-toolbar",
    "generator-wcfactory-version": "0.8.7",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/simple-toolbar.css",
      "scss": "src/simple-toolbar.scss",
      "html": "src/simple-toolbar.html",
      "js": "src/simple-toolbar.js",
      "properties": "src/simple-toolbar-properties.json",
      "hax": "src/simple-toolbar-hax.json"
    },
    "sharedHaxProps": [],
    "sharedProps": [],
    "sharedStyles": []
  },
  "version": "4.0.15",
  "description": "a customizable toolbar",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "simple-toolbar.js",
  "module": "simple-toolbar.js",
  "umd": "simple-toolbar.umd.js",
  "scripts": {
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && node --experimental-json-modules ../../node_modules/.bin/rollup --config && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"**/*.js\" --format vscode --outFile vscode-html-custom-data.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "web-dev-server -c ../../web-dev-server.config.js",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs",
    "test:watch": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox --watch",
    "test": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/a11y-menu-button": "^4.0.15",
    "@lrnwebcomponents/radio-behaviors": "^4.0.15",
    "@lrnwebcomponents/simple-icon": "^4.0.15",
    "@lrnwebcomponents/simple-tooltip": "^4.0.15",
    "lit": "^2.2.0"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "2.0.5",
    "@open-wc/testing": "3.0.4",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.6.0",
    "concurrently": "7.0.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "3.0.0",
    "lodash": "^4.17.21",
    "polymer-build": "3.1.4",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
